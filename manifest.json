{
    "name": "WebRTC IP Leak Preventer",
    "version": "1.0",
    "description": "Prevents IP Leaks",
    "permissions": [
        "storage",
        "notifications"
    ],
    "background": {
      "matches": ["http://*/*", "https://*/*"],
      "scripts": ["onstart.js"],
      "persistent": false,
      "run_at":"document_start"
    },
    "page_action": {
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      },
      "default_popup": "popup.html"
    },
    "manifest_version": 2
  }